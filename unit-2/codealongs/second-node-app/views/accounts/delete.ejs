  <body class="text-center">
    <main class="form-signin">
      <h1>Delete Account: <%= account.accountNumber %></h1>
      <h2>Are you sure ?</h2>
      <form action="/accounts/<%= account.accountNumber %>/?_method=DELETE" method="POST">
        <input type="submit" class="btn btn-danger" value="Yes" />
        <a href="/accounts/<%= account.accountNumber %>" class="btn btn-success">No</a>
      </form>
      
    </main>
  </body>
